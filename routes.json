{
  "/reports": {
    "post": {
      "controller": "reports",
      "handler": "createReport"
    },
    "get": {
      "controller": "reports",
      "handler": "getAllReports"
    }
  },
  "/reports/:id": {
    "get": {
      "controller": "reports",
      "handler": "getReportById"
    },
    "put": {
      "controller": "reports",
      "handler": "updateReport"
    }
  }
}